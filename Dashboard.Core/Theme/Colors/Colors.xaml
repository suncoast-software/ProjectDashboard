<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Colors -->
    <Color x:Key="Primary">#2c2c2c</Color>
    <Color x:Key="Secondary">#3c3c3c</Color>
    <Color x:Key="Accent">#3cfc02</Color>
    
    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryColorBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource Accent}"/>

    <!-- Gradients -->
    
</ResourceDictionary>